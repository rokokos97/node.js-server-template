{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
  "eslint:recommended",
  "plugin:@typescript-eslint/recommended",
  "plugin:prettier/recommended"
],
  "parserOptions": {
  "ecmaVersion": 2020,
    "sourceType": "module"
},
  "rules": {
  "semi": ["error", "always"],
  "@typescript-eslint/explicit-function-return-type": "off",
  "prettier/prettier": "error"
}
}
